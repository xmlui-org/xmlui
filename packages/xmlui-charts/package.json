{
  "name": "xmlui-charts",
  "version": "0.1.10",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start": "xmlui start",
    "build:demo": "xmlui build",
    "preview": "xmlui preview",
    "build": "xmlui build-lib",
    "build-watch": "xmlui build-lib --watch",
    "build:meta": "xmlui build-lib --mode=metadata",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.7",
    "classnames": "^2.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xmlui": "*"
  },
  "main": "./dist/xmlui-charts.js",
  "module": "./dist/xmlui-charts.mjs",
  "exports": {
    ".": {
      "import": "./dist/xmlui-charts.mjs",
      "require": "./dist/xmlui-charts.js"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
