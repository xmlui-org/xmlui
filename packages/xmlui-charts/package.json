{
  "name": "xmlui-charts",
  "version": "0.1.16",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start": "xmlui start",
    "build:demo": "xmlui build",
    "preview": "xmlui preview",
    "build:extension": "xmlui build-lib",
    "build-watch": "xmlui build-lib --watch",
    "build:meta": "xmlui build-lib --mode=metadata"
  },
  "dependencies": {
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "xmlui": "*"
  },
  "main": "./dist/xmlui-charts.js",
  "module": "./dist/xmlui-charts.mjs",
  "exports": {
    ".": {
      "import": "./dist/xmlui-charts.mjs",
      "require": "./dist/xmlui-charts.js"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
