{
  "name": "xmlui-editor-demo",
  "version": "0.0.1",
  "author": "norbidotdev",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    },
    "./json-client": {
      "types": "./dist/json/client/wrapperWs.d.ts",
      "default": "./dist/json/client/wrapperWs.js"
    },
    "./python-client": {
      "types": "./dist/python/client/main.d.ts",
      "default": "./dist/python/client/main.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index"
      ],
      "node": [
        "dist/node"
      ],
      "json-client": [
        "dist/json/client/wrapperWs"
      ],
      "python-client": [
        "dist/python/client/main"
      ]
    }
  },
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=10.2.3"
  },
  "files": [
    "dist",
    "src",
    "*.html",
    "*.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "dependencies": {
    "vscode-languageclient": "~9.0.1",
    "@codingame/esbuild-import-meta-url-plugin": "^1.0.3",
    "@codingame/monaco-vscode-lifecycle-service-override": "~15.0.2",
    "@codingame/monaco-vscode-editor-api": "~15.0.2",
    "@codingame/monaco-vscode-explorer-service-override": "~15.0.2",
    "@codingame/monaco-vscode-rollup-vsix-plugin": "^15.0.3",
    "@codingame/monaco-vscode-search-result-default-extension": "~15.0.2",
    "@codingame/monaco-vscode-search-service-override": "~15.0.2",
    "@codingame/monaco-vscode-textmate-service-override": "~15.0.2",
    "@codingame/monaco-vscode-theme-defaults-default-extension": "~15.0.2",
    "@codingame/monaco-vscode-theme-service-override": "~15.0.2",
    "@typefox/monaco-editor-react": "~6.6.0",
    "@vitejs/plugin-react": "^4.3.4",
    "monaco-editor-wrapper": "~6.6.0",
    "monaco-languageclient": "~9.5.0",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "vscode": "npm:@codingame/monaco-vscode-extension-api@~15.0.2",
    "xmlui": "*"
  },
  "devDependencies": {
    "vscode-languageserver-types": "~3.17.5"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "tsc --noEmit && vite build",
    "compile": "tsc "
  }
}
