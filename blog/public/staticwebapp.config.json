{
  "mimeTypes": {
    ".md": "text/markdown; charset=utf-8",
    ".json": "application/json",
    ".rss": "application/rss+xml"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/blog/*",
      "/blog/**/*",
      "/resources/*",
      "/*.css",
      "/*.js",
      "/*.json",
      "/*.svg",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.ico",
      "/*.woff",
      "/*.woff2",
      "/*.ttf",
      "/*.eot",
      "/*.rss"
    ]
  }
}
