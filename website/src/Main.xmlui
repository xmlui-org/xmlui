      <HStack gap="0">
        <NavLink
          noIndicator
          label="Home"
          to="/"
          active="{currentSegment === '/'}"
        />
        <NavLink
          noIndicator
          label="Docs"
          to="/docs"
          active="{currentSegment.startsWith('/docs')}"
        />
        <NavLink
          noIndicator
          label="Blog"
          to="/blog"
          active="{currentSegment.startsWith('/blog')}"
        />
        <NavLink
          to="/get-started"
          noIndicator
          label="Get started"
        />
      </HStack>
    </Stack>
    <SpaceFiller />
    <Search
      collapsible="true"
      when="{mediaSize.sizeIndex > 1}"
      data="{appGlobals.plainTextContent}"
    />
  </AppHeader>
