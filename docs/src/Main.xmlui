<App
    layout="vertical-full-header"
    noScrollbarGutters="false"
>
    <AppHeader>
        <property name="logoTemplate">
            <Link to="https://xmlui.org/">
                <Logo height="$space-8" />
            </Link>
        </property>
        <HStack when="{mediaSize.sizeIndex > 2}" gap="0">
            <NavLink noIndicator label="Docs" to="/" active />
            <NavLink noIndicator label="Blog" to="https://blog.xmlui.org" />
            <NavLink
                to="https://www.xmlui.org/get-started"
                noIndicator
                label="Get started"
            />
        </HStack>
        <SpaceFiller />
        <Search
            collapsible="true"
            when="{mediaSize.sizeIndex > 1}"
            data="{appGlobals.plainTextContent}"
        />
    </AppHeader>
    <NavPanel
        when="{(!globalThis.location.hash.includes('/404') && !globalThis.location.pathname.includes('/404')) || mediaSize.sizeIndex <= 2}"
        scrollStyle="whenScrolling"
        showScrollerFade="true"
    >
        <Stack
            paddingHorizontal="$space-4"
            paddingBottom="$space-4"
            when="{mediaSize.sizeIndex <= 2}"
        >
            <Search data="{appGlobals.plainTextContent}" />
        </Stack>
        <Fragment when="{mediaSize.sizeIndex <= 2}">
            <NavLink
                noIndicator
                label="Demo"
                to="https://demo.xmlui.org"
            />
            <NavLink
                noIndicator
                label="Docs"
                to="/"
                active
            />
            <NavLink
                noIndicator
                label="Blog"
                to="https://blog.xmlui.org"
            />
            <NavLink
                to="https://xmlui.org/news"
                label="News & Reviews"
                target="_self"
                noIndicator
            />
            <NavLink
                to="/change-log"
                label="Changelog"
                noIndicator
                target="_self"
            />
            <NavLink
                to="https://github.com/xmlui-org/xmlui"
                icon="github"
                label="GitHub"
                noIndicator
                target="_blank"
            />
            <NavLink
                to="https://www.xmlui.org/get-started"
                noIndicator
                label="Download"
            />
        </Fragment>
        <Stack
            padding="0"
            paddingTop="{mediaSize.sizeIndex <= 2 ? '$space-4' : '0'}"
            gap="0"
        >
            <NavGroup initiallyExpanded="{true}" label="Learn XMLUI" to="/learn">
