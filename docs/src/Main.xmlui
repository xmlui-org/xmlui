            </script>
            <DocumentPage when="{content}" content="{content}" />
            <Redirect when="{!content}" to="/404" />
        </Page>
        <Page url="/extensions/*">
            <script>
                var contentKey = 'extensions/' + $routeParams['*'];
                var content = appGlobals.content[contentKey];
            </script>
            <DocumentPage when="{content}" content="{content}" />
            <Redirect when="{!content}" to="/404" />
        </Page>
        <Page url="/404">
            <PageNotFound />
        </Page>
    </Pages>
    <Footer>
        <Text variant="em">
            This site is an XMLUI™ app.
        </Text>
        <SpaceFiller />
        <FooterLinks />
        <SpaceFiller />
        <ToneSwitch />
    </Footer>
</App>