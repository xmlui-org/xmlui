        'adduser', 'admonition_danger', 'admonition_info',
        'admonition_note', 'admonition_tip', 'admonition_warning', 'api', 'apps',
        'arrowdown', 'arrowleft', 'arrowright', 'arrowup', 'assign', 'attach',
        'attach2', 'binding', 'board', 'box', 'chat', 'checkmark', 'chevrondown',
        'chevronleft', 'chevronright', 'chevronup', 'close', 'cog', 'code',
        'codesandbox', 'compactlist', 'conf', 'connect', 'copy',
        'cube', 'date', 'database', 'darkToLight', 'doc', 'doc-outline', 'docx',
        'dotmenu', 'dotmenuhorizontal', 'download', 'drive', 'duplicate',
        'emptyfolder', 'emoji', 'error', 'exit', 'expression', 'filledplus',
        'filter', 'folder', 'folder-outline', 'gif', 'globe', 'hash', 'help',
        'home', 'hyperlink', 'info', 'inspect', 'jpeg', 'jpg', 'like',
        'link', 'list', 'looseList', 'lock', 'message', 'moon', 'move',
        'new-window', 'next', 'noresult', 'note', 'nosort', 'options', 'paint',
        'palette', 'pause', 'pen', 'permissions', 'photo', 'phone', 'play',
