{
  "name": "xmlui-docs",
  "private": true,
  "version": "0.0.14",
  "scripts": {
    "start": "echo xmlui start",
    "preview": "xmlui preview",
    "gen:releases": "node scripts/get-releases.js --output 'public/resources/files/releases.json'",
    "gen:download-latest-xmlui-release": "node scripts/download-latest-xmlui.js",
    "gen:rss": "node scripts/generate-rss.js",
    "gen:sitemap": "tsx scripts/generate-sitemap.mjs",
    "build:docs": "npm run gen:releases && npm run gen:download-latest-xmlui-release && npm run gen:rss && npm run gen:sitemap && xmlui build --buildMode=INLINE_ALL --withMock ",
    "build-optimized": "npm run gen:releases && npm run gen:download-latest-xmlui-release && npm run gen:rss && npm run gen:sitemap && npx xmlui-ssg",
    "release:docs": "xmlui zip-dist --source=dist --target=dist/ui.zip",
    "release-ci-optimized": "npm run build-optimized && mv xmlui-optimized-output/optimized-staticwebapp.config.json xmlui-optimized-output/staticwebapp.config.json",
    "preview-ssg": "npx preview-ssg ./xmlui-optimized-output"
  },
  "dependencies": {
    "xmlui": "*",
    "xmlui-playground": "*",
    "xmlui-search": "*",
    "xmlui-hello-world": "*",
    "xmlui-website-blocks": "*",
    "xmlui-docs-blocks": "*"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "devDependencies": {
    "xmlui-website-blocks": "*",
    "xmlui-animations": "*",
    "@emotion/is-prop-valid": "^1.3.1",
    "@octokit/rest": "^22.0.0"
  }
}
