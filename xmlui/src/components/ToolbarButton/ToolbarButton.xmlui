<Component name="ToolbarButton">
  <Button
    variant="ghost"
    themeColor="secondary"
    horizontalPadding="$padding-horizontal-ToolbarButton"
    verticalPadding="$padding-vertical-ToolbarButton"
    icon="{$props.icon}"
    label="{$props.label}"
    onClick="e => emitEvent('click', e)">
    <Slot />
  </Button>
</Component>