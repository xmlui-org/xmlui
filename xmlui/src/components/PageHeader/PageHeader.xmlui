<Component name="PageHeader">
  <HStack gap="$gap-PageHeader" verticalAlignment="center">
    <VStack gap="$space-tight">
      <Text when="{$props.preTitle !== undefined}" variant="subheading" value="{$props.preTitle}" />
      <H2 value="{$props.title}" />
    </VStack>
    <SpaceFiller />
    <Slot />
  </HStack>
</Component>
