    "@tanstack/react-query-devtools": "^4.36.1",
    "@tanstack/react-table": "8.17.3",
    "@tanstack/react-virtual": "3.10.8",
    "@types/chroma-js": "^3.1.1",
    "@types/color": "3.0.6",
    "@vitejs/plugin-react": "4.3.0",
    "adm-zip": "0.5.10",
    "axios": "1.8.2",
    "chroma-js": "^3.1.2",
    "classnames": "2.5.1",
    "cmdk": "^1.0.4",
    "color": "4.2.3",
    "date-fns": "2.30.0",
    "dexie": "3.2.4",
